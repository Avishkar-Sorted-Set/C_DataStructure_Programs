#include<stdio.h>
#include<stdlib.h>

typedef struct Movie {
	char mName[20];
	float imdb;
	struct Movie *next;
}mov;
struct Movie *head;
void addNode() {
	mov *newnode = (mov*)malloc(sizeof(mov));
	printf("Enter Movie Name\n");
	fgets(newnode->mName,15,stdin);
	printf("Enter imdb\n");
	scanf("%f",&newnode->imdb);
	newnode->next=NULL;
	getchar();

	if(head == NULL) {
		head = newnode;
	}else {
		mov *temp = head;
		while(temp!=NULL) {
			temp=temp->next;
		}
		temp->next = newnode;
	}
}
void printLL() {
	mov *temp = head;
	while(temp!=NULL) {
		printf("%s",temp->mName);
		printf("%f",temp->imdb);
		temp = temp->next;
	}
}
void main() {

	addNode();
	addNode();
	addNode();
	addNode();

	printLL();

}


